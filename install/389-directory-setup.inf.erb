[General] 
FullMachineName= <%= hostname+"."+domain %>
SuiteSpotUserID= nobody 
SuiteSpotGroup= nobody 
AdminDomain= <%= domain %>
ServerRoot = /usr/lib64/dirsrv
ConfigDirectoryAdminID= <%= creds["cfg_directory_adm"]["user"] %> 
ConfigDirectoryAdminPwd= <%= creds["cfg_directory_adm"]["pw"] %> 
ConfigDirectoryLdapURL= ldap://<%= hostname+"."+domain %>:389/o=NetscapeRoot 

[admin] 
Port= 9830
ServerIpAddress= 0.0.0.0
ServerAdminID= <%= creds["admin_svr_user"]["user"] %> 
ServerAdminPwd= <%= creds["admin_svr_user"]["pw"] %>

[slapd] 
AddOrgEntries = Yes
AddSampleEntries = No
SlapdConfigForMC= Yes 
UseExistingMC= No 
ServerPort= 389 
ServerIdentifier= <%= hostname %> 
Suffix= <%= domain_dn %>
naming_value= <%= domain %>
RootDN= <%= creds["root_dn_user"]["user"] %>
RootDNPwd= <%= creds["root_dn_user"]["pw"] %>
