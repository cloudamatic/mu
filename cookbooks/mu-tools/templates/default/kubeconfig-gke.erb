apiVersion: v1
clusters:
- cluster:
    server: <%= @endpoint %>
    certificate-authority-data: <%= @cacert %>
  name: <%= @cluster %>cluster
kind: Config
preferences: {}
contexts:
- context:
    cluster: <%= @cluster %>cluster
    user: <%= @cluster %>user
  name: <%= @cluster %>context
current-context: <%= @cluster %>context
users:
- name: <%= @cluster %>user
  user:
    client-certificate-data: <%= @clientcert %>
    client-key-data: <%= @clientkey %>
